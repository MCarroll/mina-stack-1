check process <%= sidekiq_name %>
  with pidfile <%= sidekiq_pid %>
  start program = "<%= sidekiq_start %>" with timeout 90 seconds
  stop program = "kill -s TERM `cat <%= sidekiq_config %>`" with timeout 90 seconds
  if totalmem is greater than 200 MB for 2 cycles then restart # eating up memory?
  group <%= sidekiq_name %>